    "babel-preset-stage-0": "^6.24.1",
    "canvas-prebuilt": "^1.6.5-prerelease.1",
    "css-loader": "^1.0.0",
    "expect": "^23.3.0",
    "file-loader": "^1.1.11",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jsdom": "^11.11.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.2.0",
    "mocha-webpack": "^2.0.0-beta.0",
    "nyc": "^12.0.2",
    "vue": "^2.5.16",
    "vue-loader": "^14.2.3",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.15.1",
    "webpack-dev-server": "^3.1.4",
